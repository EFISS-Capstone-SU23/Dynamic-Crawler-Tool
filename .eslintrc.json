{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "indent": ["error", "tab"],
        "no-tabs": "off",
        "import/prefer-default-export": "off",
        "no-await-in-loop": "off",
        "no-console": "off",
        "no-async-promise-executor": "off",
        "no-plusplus": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
